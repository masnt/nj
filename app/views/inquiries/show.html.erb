<%# rendering header(admin) %>
<div class="container">
	<div class="row "><p class="lead">お問い合わせ詳細</p></div>

	<div>受診日</div>
    <div>タイトル</div>
    <div>ユーザー名</div>
    <div>内容</div>
    <div>管理者返信内容</div>
    <div>返信する</div>
    <div>ユーザーメールアドレス</div>
    <div>返信本文</div>
    <div>送信ボタンsubmit</div>

    <%#= form_for @inquiry do |f| %>
	    <div><%#= f.inquiry_title %></div>
	    <div><%#= f.inquiry_title %></div>
	    <div><%#= f.user_name %></div>
